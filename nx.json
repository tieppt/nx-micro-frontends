{
  "npmScope": "mfes",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "calendar": {
      "tags": []
    },
    "calendar-e2e": {
      "tags": [],
      "implicitDependencies": ["calendar"]
    },
    "mailbox": {
      "tags": []
    },
    "mailbox-e2e": {
      "tags": [],
      "implicitDependencies": ["mailbox"]
    },
    "shell": {
      "tags": []
    },
    "shell-e2e": {
      "tags": [],
      "implicitDependencies": ["shell"]
    }
  }
}
